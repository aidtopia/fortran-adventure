C   SCRATCH PROJECT TO CHECK OUT THE TRANSLATOR

      PROGRAM ADVTST

      IMPLICIT INTEGER(A-Z)
      EXTERNAL SAYIT,NOCALL
      DIMENSION D(2)
      DATA X/32/,W1/'HELLO'/,W2/'WORLD'/,D/'X','X'/

      TYPE 55,W1,W2
55    FORMAT(' ',A5,', ',A5,'!')
      CALL DATE(D)
      TYPE 66,D
66    FORMAT(' TODAY IS ',2A5)
77    FORMAT(' THIS IS AN UNUSED FORMAT STATEMENT')
      GOTO 99
88    UNUSED='NOPE'
99    CALL FOO(BAR(X), SAYIT)
      STOP
      END


      SUBROUTINE FOO(X, SUB)

      IMPLICIT INTEGER(A-Z)

      CALL SUB(X)
      END


      FUNCTION BAR(X)

      IMPLICIT INTEGER(A-Z)

      BAR = X
      RETURN
      END
      

      SUBROUTINE NOCALL(X, Y, Z)

      IMPLICIT INTEGER(A-Z)
      
      TYPE 55,X,Y
55    FORMAT(' COORDINATE = (',I3,', ', I3,')')
      END

      SUBROUTINE SAYIT(X)

      IMPLICIT INTEGER(A-Z)

      TYPE 55,X
55    FORMAT(/' YOUR NUMBER IS: ',I3)
      END
